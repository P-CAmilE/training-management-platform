<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mycharge.trainingmanagementplatform.mapper.AMapper">

    <insert id="save"  useGeneratedKeys="true" keyProperty="aid" parameterType="com.mycharge.trainingmanagementplatform.model.A">
        insert A (aname) values (#{aname})
    </insert>

    <select id="findByAname" resultType="com.mycharge.trainingmanagementplatform.model.A" parameterType="String" >
        select * from A where aname=#{aname,jdbcType=VARCHAR}
    </select>
</mapper>
